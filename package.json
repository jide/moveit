    "eslint-plugin-filenames": "*",
    "eslint-plugin-react": "*",
    "jasmine": "*",
    "json-loader": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-jasmine": "*",
    "karma-spec-reporter": "*",
    "karma-webpack": "*",
    "rimraf": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
